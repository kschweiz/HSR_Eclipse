import java.util.concurrent.SynchronousQueue;

public class Test {

	public static void main(String[] args) {
		Kaffe kaffe = new Espreso();
		kaffe.print();

	}

}


class Kaffe{
	public void print() {
		System.out.println("Kaffe");
	}
}

class Espreso extends Kaffe{
	public void print() {
		System.out.println("Espresso");
	}
}
